@baseUrl = http://localhost:5001
@urlPath = api/v1

###
POST {{baseUrl}}/{{urlPath}}/post
Content-Type: application/json
Accept: application/json
withCredentials: true

# Privacy - Public | Private | Followers
# Feelings - like | love | wow | happy | sad | angry

{
  "post": "this is my posttt",
  "bgColor": "#er23234aaa",
  "privacy": "Public",
  "gifUrl": "",
  "profilePicture": "data:image/jpeg;base64,",
  "feelings": "happy"
}

###
POST {{baseUrl}}/{{urlPath}}/post/image/post
Content-Type: application/json
Accept: application/json
withCredentials: true


{
  "post": "this is my post qith image",
  "bgColor": "#er23234",
  "privacy": "Public",
  "gifUrl": "",
  "profilePicture": "<cloudinary-user-profile-picture-url>",
  "feelings": "happy",
  "image": "data:image/jpeg;base64,"
}


